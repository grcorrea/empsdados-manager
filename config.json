{
  "app": {
    "title": "Emps ados Manager",
    "theme_mode": "dark",
    "window": {
      "width": 600,
      "height": 750,
      "resizable": false
    }
  },
  "s3": {
    "prefix_options": [
      "local-developers",
      "local-users"
    ],
    "rt_options": [
      "fluxo",
      "corebank",
      "assessoria",
      "credito"
    ],
    "rt_squad_hierarchy": {
      "fluxo": [
        "boleto",
        "recorrente"
      ],
      "corebank": [
        "iam",
        "foundation"
      ],
      "assessoria": [],
      "credito": []
    },
    "environment_options": [
      "sirius",
      "athena"
    ],
    "default_base_path": "s3",
    "current_selections": {
      "prefix": "local-developers",
      "rt": "fluxo",
      "squad": "boleto",
      "env": "sirius"
    }
  },
  "aws": {
    "config_files": [
      "config",
      "config.txt"
    ],
    "default_profile": "default"
  }
}