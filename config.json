{
  "app": {
    "title": "EMPS Dados Manager",
    "theme_mode": "dark",
    "window": {
      "width": 800,
      "height": 800,
      "resizable": true
    }
  },
  "s3": {
    "prefix_options": [
      "local-developers",
      "local-users"
    ],
    "rt_options": [
      "fluxo",
      "corebank",
      "assessoria",
      "credito"
    ],
    "rt_squad_hierarchy": {
      "fluxo": [
        "boleto",
        "adquirencia",
        "cobranca",
        "pix",
        "recorrente"
      ],
      "corebank": [
        "iam",
        "foundation"
      ],
      "assessoria": [],
      "credito": []
    },
    "environment_options": [
      "sirius",
      "athena",
      "reprocess"
    ],
    "default_base_path": "s3",
    "current_selections": {
      "prefix": "local-developers",
      "rt": "fluxo",
      "squad": "boleto",
      "env": "sirius"
    }
  },
  "aws": {
    "config_files": [
      "config",
      "config.txt"
    ],
    "default_profile": "default"
  },
  "glue": {
    "databases": [
      "workspace_db"
    ]
  },
  "filters": {
    "glue_monitoring": "",
    "stp_monitoring": "",
    "table_monitoring": "",
    "eventbridge_monitoring": ""
  }
}